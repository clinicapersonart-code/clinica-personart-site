{
    "buildCommand": "pnpm build",
    "outputDirectory": "dist/public",
    "installCommand": "pnpm install",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/trpc/:path*",
            "destination": "/api/trpc"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}